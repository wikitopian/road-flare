{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/road-flare-react/build",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "road-flare",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/road-flare-react/public/images/road-flare.png"
    }
  },
  "cli": {
    "binaryName": "road-flare",
    "resourcesPath": "/road-flare-react/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/road-flare-react/public/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0"
  }
}
